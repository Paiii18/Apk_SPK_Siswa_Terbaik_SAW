	<queryString language="SQL">
		<![CDATA[SELECT 
    a.nama_siswa,
    ha.skor_akhir,
    (
        SELECT COUNT(*) + 1 
        FROM hasil_akhir ha2
        WHERE ha2.skor_akhir > ha.skor_akhir
    ) AS ranking
FROM hasil_akhir ha
JOIN alternatif a ON ha.id_siswa = a.id_siswa
ORDER BY ranking ASC;]]>
	</queryString>
